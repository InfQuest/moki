{
  "name": "Moki: Learning Language in the Youtube",
  "description": "Make anything you watch a language lesson.",
  "version": "0.0.9",
  "manifest_version": 3,
  "default_locale": "en",
  "icons": {
    "128": "img/logo-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/logo-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-B5W4wO-3.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "assets/index-BP3GEb2z.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "img/logo-128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/injected.js",
        "assets/injected.js.map",
        "_locales/*/messages.json",
        "assets/chunk--DVCTphW.js",
        "assets/chunk--DVCTphW.js.map",
        "assets/chunk-Ds9tf01o.js",
        "assets/chunk-Ds9tf01o.js.map",
        "assets/chunk-BAX9q0V5.js",
        "assets/chunk-BAX9q0V5.js.map",
        "assets/chunk-BD1GsdAo.js",
        "assets/chunk-BD1GsdAo.js.map",
        "assets/chunk-COyM46jY.js",
        "assets/chunk-COyM46jY.js.map"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://ext.vocay.com/*"
  ]
}
