{
  "key": "",
  "name": "Moki: Learn Languages on YouTube",
  "description": "Make anything you watch a language lesson.",
  "version": "0.1.2",
  "manifest_version": 3,
  "default_locale": "en",
  "icons": {
    "128": "img/logo-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/logo-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-CTS3hK0u.js"
      ],
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "css": [
        "assets/index-DjGT5wwA.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "img/logo-128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "resources": [
        "assets/injected.js",
        "_locales/*/messages.json",
        "assets/chunk-DVDiAI92.js",
        "assets/chunk-BHmWp2Sy.js",
        "assets/chunk-cFAJfbYw.js",
        "assets/chunk-Cpj98o6Y.js",
        "assets/chunk-BgQDa6bu.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "identity"
  ],
  "host_permissions": [
    "https://api.mokiedu.com/*",
    "https://static.infquest.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://mokiedu.com/*",
      "https://*.mokiedu.com/*",
      "http://localhost:3000/*"
    ]
  }
}
