{
  "name": "Moki: Learning Language in the Youtube",
  "description": "Make anything you watch a language lesson.",
  "version": "0.1.0",
  "manifest_version": 3,
  "default_locale": "en",
  "icons": {
    "128": "img/logo-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/logo-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-loader-BlIWxLMN.js"
      ],
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "css": [
        "assets/index-DDd5_SkI.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "img/logo-128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "resources": [
        "assets/injected.js",
        "_locales/*/messages.json",
        "assets/chunk-B-UrLAwj.js",
        "assets/chunk-CZRYylKx.js",
        "assets/chunk-DwgIagi8.js",
        "assets/chunk-Cpj98o6Y.js",
        "assets/chunk-1Y0sDltK.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://ext.vocay.com/*"
  ]
}
